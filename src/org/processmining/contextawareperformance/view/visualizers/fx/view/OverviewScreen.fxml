<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <center>
      <FlowPane fx:id="contentPane" hgap="10.0" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" vgap="10.0" />
   </center>
   <top>
      <HBox maxHeight="1.7976931348623157E308" prefHeight="135.0" BorderPane.alignment="CENTER">
         <children>
            <VBox maxHeight="1.7976931348623157E308" prefWidth="500.0" HBox.hgrow="ALWAYS">
               <children>
                  <Label fx:id="lblTitle" text="Analysis results">
                     <font>
                        <Font name="System Bold" size="16.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblDescription" text="Below, you'll find the results of the context-aware process performance analysis. Click on an item to see detailed information about the discovered results for the selected performance metric." wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font></Label>
                  <Label fx:id="lblUnsignificantPerformanceFunctions" text="No significant results for:" wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="10.0" />
                     </VBox.margin>
                  </Label>
                  <Label fx:id="lblUnsignificantContextFunctions" text="No significant results for:" wrapText="true">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
            </VBox>
            <Region minWidth="10.0" prefWidth="200.0" HBox.hgrow="SOMETIMES" />
            <VBox alignment="TOP_RIGHT" minWidth="200.0" HBox.hgrow="NEVER">
               <children>
                  <Label fx:id="lblNrSignificantResults" text="significant results: 0">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblNrSignificantPerformanceFunctions" text="performance functions: 0" textAlignment="RIGHT">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
                  <Label fx:id="lblNrSignificantContextFunctions" text="context functions: 0" textAlignment="RIGHT">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Label>
               </children>
               <HBox.margin>
                  <Insets top="20.0" />
               </HBox.margin>
            </VBox>
         </children>
      </HBox>
   </top>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</BorderPane>
